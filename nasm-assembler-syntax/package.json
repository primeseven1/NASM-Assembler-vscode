{
  "name": "nasm-assembler-syntax",
  "displayName": "NASM Assembler Syntax",
  "description": "NASM assembler syntax highlighting",
  "repository": "https://github.com/primeseven1/NASM-Assembler-vscode",
  "publisher": "primeseven",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nasm",
      "aliases": ["NASM", "nasm"],
      "extensions": [".asm",".nasm",".s",".ASM",".NASM",".S"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "nasm",
      "scopeName": "source.nasm",
      "path": "./syntaxes/nasm.tmLanguage.json"
    }]
  }
}
